{
  "name": "holomind-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn lint:watch & yarn type-check --watch & cross-env BUILD_ENV=development vite",
    "start:https": "yarn lint:watch & yarn type-check --watch & cross-env BUILD_ENV=development vite",
    "setup:https": "bash scripts/setup-https.sh",
    "icons": "node scripts/generate-icon-fonts.js",
    "generate": "wagmi generate",
    "build:test": "yarn type-check && cross-env BUILD_ENV=test vite build",
    "build": "yarn type-check && cross-env BUILD_ENV=production vite build",
    "build:analyze": "yarn type-check && cross-env BUILD_ENV=production ANALYZE=true vite build",
    "analyze": "yarn build:analyze && npx vite-bundle-analyzer dist",
    "analyze:size": "node analyze-bundle.js",
    "build:size": "yarn build && yarn analyze:size",
    "lint": "eslint .",
    "lint:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"eslint .\"",
    "preview": "vite preview",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "type-check": "tsc -b --force",
    "i18n:start": "yarn extract && yarn compile",
    "i18n:compile": "yarn extract --locale en-US && yarn compile",
    "i18n:compile:local": "yarn compile",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:src": "prettier --write src/",
    "format:staged": "prettier --write --list-different",
    "install:hooks": "./.githooks/install-hooks.sh"
  },
  "dependencies": {
    "@lingui/cli": "^5.3.0",
    "@lingui/core": "^5.3.0",
    "@lingui/macro": "^5.3.0",
    "@lingui/react": "^5.3.0",
    "@msgpack/msgpack": "^3.1.2",
    "@popperjs/core": "^2.11.8",
    "@reach/dialog": "^0.18.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@reown/appkit": "^1.8.12",
    "@reown/appkit-adapter-solana": "^1.8.12",
    "@reown/appkit-adapter-wagmi": "^1.8.12",
    "@reown/appkit-wallet-button": "^1.8.12",
    "@tanstack/react-query": "^5.90.5",
    "@types/qs": "^6.9.18",
    "@types/styled-components": "^5.1.34",
    "@xyflow/react": "^12.10.0",
    "bignumber.js": "^9.2.0",
    "boring-avatars": "^1.11.2",
    "bs58": "^6.0.0",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "eruda": "^3.4.3",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "json-bigint": "^1.0.0",
    "lightweight-charts": "^5.0.6",
    "lottie-web": "^5.13.0",
    "ogl": "^1.0.11",
    "postprocessing": "^6.38.0",
    "qrcode.react": "^4.2.0",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.5.6",
    "react-markdown": "^10.1.0",
    "react-popper": "^2.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.0",
    "react-toastify": "^11.0.5",
    "react-use-websocket": "^4.13.0",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.86.3",
    "styled-components": "^6.1.17",
    "three": "^0.182.0",
    "ua-parser-js": "^2.0.3",
    "viem": "^2.38.5",
    "wagmi": "^2.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@wagmi/cli": "^2.8.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fantasticon": "^4.1.0",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "serve": "^14.2.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "packageManager": "yarn@1.22.22"
}
