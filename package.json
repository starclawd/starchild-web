{
  "name": "holomind-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn lint:watch & yarn type-check --watch & cross-env BUILD_ENV=development vite",
    "build:test": "yarn compile && yarn type-check && cross-env BUILD_ENV=test vite build",
    "build": "yarn compile && yarn type-check && cross-env BUILD_ENV=production vite build",
    "lint": "eslint .",
    "lint:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"eslint .\"",
    "preview": "vite preview",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "type-check": "tsc -b --force",
    "i18n:start": "yarn extract && yarn compile",
    "i18n:compile": "yarn extract --locale en-US && yarn compile",
    "prepare": "yarn i18n:compile"
  },
  "dependencies": {
    "@lingui/cli": "^5.3.0",
    "@lingui/core": "^5.3.0",
    "@lingui/macro": "^5.3.0",
    "@lingui/react": "^5.3.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@popperjs/core": "^2.11.8",
    "@reach/dialog": "^0.18.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/qs": "^6.9.18",
    "@types/styled-components": "^5.1.34",
    "bignumber.js": "^9.2.0",
    "boring-avatars": "^1.11.2",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "html2canvas": "^1.4.1",
    "json-bigint": "^1.0.0",
    "lightweight-charts": "^5.0.6",
    "make-plural": "^7.4.0",
    "pako": "^2.1.0",
    "qrcode.react": "^4.2.0",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-popper": "^2.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.0",
    "react-toastify": "^11.0.5",
    "react-use-websocket": "^4.13.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.86.3",
    "styled-components": "^6.1.17",
    "ua-parser-js": "^2.0.3",
    "vconsole": "^3.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-watch": "^8.0.0",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-remove-console": "^2.2.0"
  }
}
